{
  "name": "snekjs",
  "version": "1.21.1",
  "description": "snake game made with p5.js",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "http-server ./dist -p 3000 -c-1",
    "serve-prod": "http-server ./dist-prod -p 3000 -c-1",
    "test": "NODE_ENV=test mocha --require mochaSetup.cjs",
    "build": "webpack --mode=production --node-env=production",
    "build-dev": "webpack --mode=development --node-env=development",
    "deploy": "npm run build && cd bin && ./deploy.sh",
    "deploy-gh-pages": "git subtree push --prefix dist/snek-js origin gh-pages",
    "watch": "webpack --watch --node-env=development"
  },
  "browser": {
    "crypto": false
  },
  "author": "townofdon",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "color": "^4.2.3",
    "expect": "^29.7.0",
    "format-number": "^3.0.0",
    "howler": "^2.2.4",
    "p5": "^1.7.0",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-paginate": "^8.2.0",
    "react-query": "^3.39.3",
    "react-select": "^5.8.0",
    "react-share": "^5.1.0",
    "react-toggle": "^4.1.3",
    "throttleit": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@fal-works/eslint-config-p5js": "^1.0.3",
    "@types/color": "^3.0.4",
    "@types/howler": "^2.2.9",
    "@types/mocha": "^10.0.6",
    "@types/node-sass": "^4.11.7",
    "@types/p5": "^1.7.0",
    "@types/react-toggle": "^4.0.5",
    "@types/throttleit": "^1.0.2",
    "@types/uuid": "^9.0.7",
    "@webpack-cli/generators": "^3.0.7",
    "assert": "^2.1.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
