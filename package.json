    "mini-css-extract-plugin": "^2.9.0",
    "mocha": "^10.2.0",
    "node-sass": "^9.0.0",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
