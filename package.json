{
  "name": "snekjs",
  "version": "1.20.2",
  "description": "snake game made with p5.js",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "http-server ./dist -p 3000 -c-1",
    "serve-prod": "http-server ./dist-prod -p 3000 -c-1",
    "test": "NODE_ENV=test mocha --require mochaSetup.cjs",
    "build": "webpack --mode=production --node-env=production",
    "build-dev": "webpack --mode=development --node-env=development",
    "deploy": "npm run build && cd bin && ./deploy.sh",
    "deploy-gh-pages": "git subtree push --prefix dist/snek-js origin gh-pages",
    "watch": "webpack --watch --node-env=development"
  },
  "browser": {
    "crypto": false
  },
  "author": "townofdon",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "color": "^4.2.3",
    "format-number": "^3.0.0",
    "howler": "^2.2.4",
    "p5": "^1.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@fal-works/eslint-config-p5js": "^1.0.3",
    "@types/color": "^3.0.4",
    "@types/howler": "^2.2.9",
    "@types/mocha": "^10.0.6",
    "@types/p5": "^1.7.0",
    "@types/uuid": "^9.0.7",
    "@webpack-cli/generators": "^3.0.7",
    "assert": "^2.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "jsdom": "24.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
