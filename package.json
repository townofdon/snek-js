{
  "name": "snekjs",
  "version": "1.5.0",
  "description": "snake game made with p5.js",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "http-server ./dist -p 3000 -c-1",
    "serve-prod": "http-server ./dist-prod -p 3000 -c-1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode=production --node-env=production",
    "deploy": "npm run build && cd bin && ./deploy.sh",
    "deploy-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "watch": "webpack --watch --node-env=development"
  },
  "author": "townofdon",
  "license": "MIT",
  "dependencies": {
    "color": "^4.2.3",
    "howler": "^2.2.4",
    "p5": "^1.7.0"
  },
  "devDependencies": {
    "@fal-works/eslint-config-p5js": "^1.0.3",
    "@types/color": "^3.0.4",
    "@types/howler": "^2.2.9",
    "@types/p5": "^1.7.0",
    "@webpack-cli/generators": "^3.0.7",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "http-server": "^14.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
